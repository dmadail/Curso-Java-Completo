package entities;

import java.time.LocalDate;

public class Cliente {
	private String nome;
	private String email;
	private LocalDate dataNascimento;
	
	public Cliente() {
		
	}

	public Cliente(String nome, String email, LocalDate dataNascimento) {
		this.nome = nome;
		this.email = email;
		this.dataNascimento = dataNascimento;
	}
	
	
}
